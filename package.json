{
  "name": "livepeer-alerts-frontend",
  "version": "0.1.0",
  "description": "Provide pro-active alert notifications that will help LPT token holders to be updated and understand how the transcoders they are delegating the tokens to are performing in near real time.",
  "author": "Protofire @protofire",
  "license": "MIT",
  "bugs": "https://github.com/protofire/livepeer-alerts-frontend/issues",
  "keywords": [
    "ethereum",
    "blockchain",
    "livepeer"
  ],
  "dependencies": {
    "@material-ui/core": "3.1.1",
    "@material-ui/icons": "3.0.1",
    "axios": "^0.18.0",
    "big-js": "^3.1.3",
    "bn.js": "^4.11.8",
    "bootstrap": "^4.1.3",
    "chartist": "0.10.1",
    "emoji-mart": "^2.9.2",
    "ethjs-unit": "^0.1.6",
    "history": "4.7.2",
    "logdown": "^3.2.7",
    "moment": "2.22.2",
    "node-sass-chokidar": "1.3.3",
    "nouislider": "12.0.0",
    "npm-run-all": "4.1.3",
    "perfect-scrollbar": "1.4.0",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-big-calendar": "0.20.1",
    "react-bootstrap-sweetalert": "4.4.1",
    "react-chartist": "0.13.1",
    "react-copy-to-clipboard": "^5.0.1",
    "react-datetime": "2.15.0",
    "react-dom": "^16.6.3",
    "react-google-maps": "9.4.5",
    "react-jvectormap": "0.0.3",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^2.1.3",
    "react-swipeable-views": "0.13.0",
    "react-table": "6.8.6",
    "react-tagsinput": "3.19.0",
    "react-toastify": "^4.5.1",
    "validator": "^10.10.0",
    "web3": "^1.0.0-beta.37"
  },
  "devDependencies": {
    "@babel/core": "7.0.0",
    "@types/googlemaps": "3.30.13",
    "@types/markerclustererplus": "2.1.33",
    "babel-eslint": "^9.0.0",
    "coveralls": "^3.0.2",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "eslint": "5.6.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-prettier": "^3.0.0",
    "husky": "1.3.0",
    "jest": "^23.6.0",
    "prettier": "^1.15.3",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^16.6.3"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom --coverage",
    "eject": "react-scripts eject",
    "lint": "eslint . src/**/*.js",
    "lint:check": "eslint . --ext=js,jsx;  exit 0",
    "lint:fix": "eslint . --ext=js,jsx --fix;  exit 0",
    "coveralls": "cat ./coverage/lcov.info | coveralls"
  },
  "eslintConfig": {},
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/src/assets/**"
    ]
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run test && npm run lint"
    }
  }
}
